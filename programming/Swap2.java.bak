class Swap2 
{
	public static void main(String[] args) 
	{
		int a=50;
		int b=20;

		System.out.println("before swap");
		System.out.println(a);
		System.out.println(b);
		if(a<b){
         b=b-a;
		 a=a+b;
		 //System.out.println("After swap");
		 //System.out.println(a);
		 //System.out.println(b);
        }
           else if(a>b){
	           a=a-b;
	           b=a+b;
			   }
               System.out.println("after swap");
		       System.out.println(a);
		       System.out.println(b);
              
	}
}
